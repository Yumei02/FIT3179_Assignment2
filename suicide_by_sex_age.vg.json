{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A ranged dot plot that uses 'layer' to convey changing life expectancy for the five most populous countries (between 1955 and 2000).",
    "data": {"url": "Suicide_by_sex_age_dataset.csv"},
    "transform": [
      {
        "filter": {
          "field": "Age_group",
          "oneOf": ["15–19", "20–24", "25–29", "30–34", "35–39", "40–44", "45–49", "50–54", "55–59", "60–64", "65–69", "70–74", "75–79", "80–84", "85+"]
        }
      },
      {
        "filter": {
          "field": "Sex",
          "oneOf": ["Males", "Females"]
        }
      }
    ],
    "encoding": {
      "x": {
        "field": "Suicide_rate",
        "type": "quantitative",
        "title": "Suicide Rate"
      },
      "y": {
        "field": "Age_group",
        "type": "nominal",
        "title": "Age Group",
        "axis": {
          "offset": 18,
          "ticks": false,
          "minExtent": 70,
          "domain": false
        }
      }
    },
    "layer": [
      {
        "mark": "line",
        "encoding": {
          "detail": {
            "field": "Age_group",
            "type": "nominal"
          },
          "color": {"value": "#db646f"}
        }
      },
      {
        "mark": {
          "type": "point",
          "filled": true
        },
        "encoding": {
          "color": {
            "field": "Sex",
            "type": "nominal",
            "scale": {
              "domain": ["Males", "Females"],
              "range": ["#e6959c", "#911a24"]
            },
            "title": "Sex"
          },
          "size": {"value": 300},
          "opacity": {"value": 1}
        }
      }
    ]
  }